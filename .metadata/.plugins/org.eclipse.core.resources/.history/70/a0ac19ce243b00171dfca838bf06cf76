package application;
	
import javafx.application.Application;
import javafx.stage.Stage;
import javafx.scene.Scene;
import javafx.scene.layout.BorderPane;
import javafx.scene.layout.GridPane;
import javafx.scene.paint.Color;
import javafx.scene.shape.Line;


public class Main extends Application {
	@Override
	public void start(Stage primaryStage) {
		try {
			BorderPane grid = new BorderPane();
			Scene scene = new Scene(grid,290,460, Color.web("#e2e2e2"));
			scene.getStylesheets().add(getClass().getResource("application.css").toExternalForm());
			
			Line line1 = new Line(0, 43, 290, 43);
			line1.setStroke(Color.WHITE);
			
			Line line2 = new Line(0, 86, 290, 86);
			line1.setStroke(Color.WHITE);
			
			Line line3 = new Line(0, 129, 290, 129);
			line1.setStroke(Color.WHITE);
			
			Line line4 = new Line(0, 172, 290, 172);
			line1.setStroke(Color.WHITE);
			
			Line line5 = new Line(0, 215, 290, 215);
			line1.setStroke(Color.WHITE);
			
			grid.getChildren().addAll(line1, line2, line3, line4, line5);
			
			primaryStage.setScene(scene);
			primaryStage.show();
		} catch(Exception e) {
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args) {
		launch(args);
	}
}
