package Problem_2;

public class Billing {
	String patientName;
	String doctorName;
	double billingAmount;
	
	public Billing(String newDoctorName, String newPatientName) {
		doctorName = newDoctorName;
		patientName = newPatientName;
		billingAmount = 0.00;
	}
	
	public void writeOutput(String doctorName, String patientName) {
			System.out.println("Doctor: " + doctorName + "\nDoctor: " + patientName + "\nBilling Amount: " + billingAmount);
		}
	
	public Boolean compareRecord() {
		
		
		return true;
	}
	


}
